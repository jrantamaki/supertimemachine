  "repository": "http://github.com/heroku/go-getting-started",
  "buildpacks": [
    {
      "url": "https://github.com/srid/heroku-buildpack-elm.git"
    },
    {
      "url": "https://github.com/heroku/heroku-buildpack-go"
    }
  ]
}
